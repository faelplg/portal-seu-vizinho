{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb", "eslint:recommended", "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": 1,
        "react/jsx-props-no-spreading": 0,
        "react/react-in-jsx-scope": 0
    }
}